[user]
	name = Matt Miller
	email = miller.mattster@gmail.com
	signingKey = ""
[core]
	autocrlf = true
	excludesfile =
	mergeoptions = --no-edit
[alias]
	sweep = !git branch --merged | egrep -v \"(^\\*|^\\s*(master|develop|test)$)\" | xargs git branch -d
[pull]
	rebase = true
[push]
	default = simple
[remote "origin"]
	prune = true
[diff]
	tool = tortoise
[difftool]
	prompt = false
[merge]
	tool = tortoise
[mergetool]
	prompt = false
	keepBackup = false
[difftool "tortoise"]
	cmd = 'C:/Program Files/TortoiseSVN/bin/TortoiseMerge.exe' \"$LOCAL\" \"$REMOTE\"
[mergetool "tortoise"]
	cmd = 'C:/Program Files/TortoiseSVN/bin/TortoiseMerge.exe'  -base:\"$BASE\" -mine:\"$LOCAL\" -theirs:\"$REMOTE\" -merged:\"$MERGED\"
	trustExitCode = true
[gpg]
	program = gpg
[commit]
	gpgSign = false
[tag]
	forceSignAnnotated = false
