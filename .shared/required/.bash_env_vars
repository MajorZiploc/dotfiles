# trimmed git bash prompt with success/fail colors
export PS1="\[\033]0;$TITLEPREFIX:$PWD\007\]\n\[\033[35m\]\u@\h \`if [[ \$? = \"0\" ]]; then echo "\\[\\033[32m\\]"; else echo "\\[\\033[31m\\]"; fi\`\w\[\033[36m\]`__git_ps1`\[\033[0m\]\n$ "

